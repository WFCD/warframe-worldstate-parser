{
  "name": "warframe-worldstate-parser",
  "version": "0.0.0-dev",
  "description": "An Open parser for Warframe's Worldstate in Javascript",
  "keywords": [
    "warframe-worldstate",
    "warframe"
  ],
  "homepage": "https://github.com/wfcd/warframe-worldstate-parser#readme",
  "bugs": {
    "url": "https://github.com/wfcd/warframe-worldstate-parser/issues"
  },
  "repository": "github:wfcd/warframe-worldstate-parser",
  "license": "MIT",
  "author": "aliasfalse",
  "contributors": [
    "nspacestd (https://github.com/nspacestd)"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist/",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "build:docs": "jsdoc -c jsdoc-config.json -d docs",
    "fmt": "prettier -c .",
    "fmt:fix": "prettier -w .",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "prepare": "husky && npx install-peerdeps @wfcd/eslint-config@latest -S",
    "prepublishOnly": "npm run build && npm_config_yes=true npx clean-package",
    "printcov": "c8 report",
    "test": "mocha",
    "test:coverage": "c8 npm test",
    "test:integration": "npm run test -- -g 'should parse live pc worldstate data'"
  },
  "prettier": "@wfcd/eslint-config/prettier",
  "overrides": {
    "sinon-chai": {
      "chai": "^5.0.3"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/types": "^19.0.3",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/sinon-chai": "^4.0.0",
    "c8": "^10.0.0",
    "chai": "^5.0.3",
    "eslint-plugin-redos": "^4.4.5",
    "husky": "^9.0.11",
    "lint-staged": "^16.0.0",
    "mocha": "^11.0.1",
    "mocha-minimalist-reporter": "^1.1.0",
    "prettier": "^3.2.5",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.0",
    "tsdown": "^0.14.2",
    "tsx": "^4.20.4",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "warframe-worldstate-data": ">=3.1.3"
  },
  "engines": {
    "node": ">=20.6.0"
  }
}
