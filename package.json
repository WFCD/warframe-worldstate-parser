{
  "name": "warframe-worldstate-parser",
  "version": "2.8.0",
  "description": "An Open parser for Warframe's Worldstate in Javascript",
  "types": "./types/main.d.ts",
  "main": "main.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "warframe-worldstate-data": "^1.19.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/node-fetch": "^2.5.8",
    "@types/rewire": "^2.5.28",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.20.1",
    "greenkeeper-lockfile": "^1.15.1",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "precommit-hook": "^3.0.0",
    "rewire": "^5.0.0",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.5.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "test": "nyc mocha",
    "test:integration": "npm run test -- -g 'should parse live worldstate data'",
    "printcov": "nyc report",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "build-docs": "jsdoc -c jsdoc-config.json -d docs",
    "lint": "eslint main.js lib/ test/",
    "lint:fix": "eslint main.js lib/ test/ --fix",
    "prepublishOnly": "npm run build:types",
    "build:types": "tsc -p tsconfig.declaration.json",
    "validate": "npm run lint:fix && git add -u ."
  },
  "files": [
    "lib",
    "main.js",
    "LICENSE",
    "types"
  ],
  "repository": "wfcd/warframe-worldstate-parser",
  "keywords": [
    "warframe-worldstate",
    "warframe"
  ],
  "author": "aliasfalse",
  "contributors": [
    "nspacestd (https://github.com/nspacestd)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wfcd/warframe-worldstate-parser/issues"
  },
  "homepage": "https://github.com/wfcd/warframe-worldstate-parser#readme",
  "release": {
    "branch": "master"
  },
  "eslintIgnore": [
    ".github/**",
    "docs/**",
    "resources/**",
    "types/**"
  ],
  "eslintConfig": {
    "extends": "airbnb-base",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "valid-jsdoc": [
        "error",
        {
          "requireReturn": false,
          "requireReturnDescription": false,
          "preferType": {
            "String": "string",
            "Number": "number",
            "Boolean": "boolean",
            "Function": "function",
            "object": "Object",
            "date": "Date",
            "error": "Error"
          },
          "prefer": {
            "return": "returns"
          }
        }
      ],
      "no-underscore-dangle": "off",
      "strict": [
        "error",
        "safe"
      ],
      "linebreak-style": "off",
      "import/no-unresolved": 0
    }
  },
  "pre-commit": [
    "validate"
  ],
  "mocha": {
    "exit": true,
    "spec": "test/**/*.spec.js",
    "timeout": 10000
  },
  "nyc": {
    "exclude": [
      "test/**",
      "lib/DarkSector*.js",
      "lib/Alert.js",
      "lib/PersistentEnemy.js",
      "lib/GlobalUpgrade.js",
      "lib/ChallengeInstance.js",
      "lib/WeeklyChallenge.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "skip-full": true
  }
}
