{
  "name": "warframe-worldstate-parser",
  "version": "2.7.4",
  "description": "An Open parser for Warframe's Worldstate in Javascript",
  "types": "./types/worldstate.d.ts",
  "main": "main.js",
  "directories": {
    "test": "test",
    "version": "git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "json-fetch-cache": "^1.0.1",
    "node-fetch": "^2.3.0",
    "node-md-config": "^2.0.1",
    "right-pad": "^1.0.1",
    "warframe-worldstate-data": "^1.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "chai": "^4.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.18.0",
    "greenkeeper-lockfile": "^1.15.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "rewire": "^4.0.0",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0",
    "tsd-jsdoc": "^2.1.3"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "test": "npm run lint:fix && npx mocha --exit",
    "build-docs": "node_modules/.bin/jsdoc -c jsdoc-config.json -d docs",
    "lint": "npx eslint main.js lib/ test/",
