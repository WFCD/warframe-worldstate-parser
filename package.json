{
  "name": "warframe-worldstate-parser",
  "version": "2.8.0",
  "description": "An Open parser for Warframe's Worldstate in Javascript",
  "types": "./types/main.d.ts",
  "main": "main.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "peerDependencies": {
    "warframe-worldstate-data": ">=1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/node-fetch": "^2.5.8",
    "@types/rewire": "^2.5.28",
    "@types/sinon-chai": "^3.2.5",
    "@wfcd/eslint-config": "^1.1.5",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "install-peerdeps": "^2.0.3",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "precommit-hook": "^3.0.0",
    "rewire": "^6.0.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.5.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "test": "nyc mocha",
    "test:integration": "npm run test -- -g 'should parse live worldstate data'",
    "printcov": "nyc report",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "build-docs": "jsdoc -c jsdoc-config.json -d docs",
    "lint": "eslint main.js lib/ test/",
    "lint:fix": "eslint main.js lib/ test/ --fix",
    "prepublishOnly": "npm run build:types && npm_config_yes=true npx clean-package",
    "build:types": "tsc -p tsconfig.declaration.json",
    "validate": "npm run lint:fix && git add -u ."
  },
  "files": [
    "lib",
    "main.js",
    "LICENSE",
    "types"
  ],
  "repository": "wfcd/warframe-worldstate-parser",
  "keywords": [
    "warframe-worldstate",
    "warframe"
  ],
  "author": "aliasfalse",
  "contributors": [
    "nspacestd (https://github.com/nspacestd)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wfcd/warframe-worldstate-parser/issues"
  },
  "homepage": "https://github.com/wfcd/warframe-worldstate-parser#readme",
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      ["@semantic-release/exec", {
        "prepareCmd": "mv package.json.backup package.json"
      }],
      ["@semantic-release/git", {
        "assets": ["package.json"]
      }]
    ]
  },
  "eslintIgnore": [
    ".github/**",
    "docs/**",
    "resources/**",
    "types/**"
  ],
  "eslintConfig": {
    "extends": "@wfcd",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "script"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-private-methods"
    ]
  },
  "pre-commit": [
    "lint",
    "test",
    "validate"
  ],
  "prettier": "@wfcd/eslint-config/prettier",
  "mocha": {
    "exit": true,
    "spec": "test/**/*.spec.js",
    "timeout": 10000
  },
  "nyc": {
    "exclude": [
      "test/**",
      "lib/DarkSector*.js",
      "lib/Alert.js",
      "lib/PersistentEnemy.js",
      "lib/GlobalUpgrade.js",
      "lib/ChallengeInstance.js",
      "lib/WeeklyChallenge.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "skip-full": true
  },
  "clean-package": {
    "remove": [
      "devDependencies",
      "scripts",
      "release",
      "eslintIgnore",
      "eslintConfig",
      "pre-commit",
      "nyc",
      "mocha",
      "clean-package",
      "directories",
      "prettier"
    ]
  }
}
